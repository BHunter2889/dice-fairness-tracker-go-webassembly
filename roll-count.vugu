<div class="dice-side-number-tracker">
	<button @click="data.Increment()">Nat20</button>
	<div vg-html='data.Count'></div>
</div>

<style>
	.dice-side-number-tracker {
		background: #eee;
	}
</style>

<script type="application/x-go">
	type RollCountData struct {
		Count int `default:"0"`
	}
	func (data *RollCountData) Increment() {
		data.Count++
		fmt.Println(data.Count)
	}
</script>